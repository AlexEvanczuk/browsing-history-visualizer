{
  "manifest_version": 2,

  "name": "The Browser Browser",
  "description": "This extension keeps track of your browsing habits to allow you to visualize and make sense of the way you use the web.",
  "version": "1.0",
  "background": {
    "scripts": ["myscript.js"]
   },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

// TODO: remove unused permissions
  "permissions": [
    "https://secure.flickr.com/",
    "tabs",
    "http://wwww.browsing-history-visualizer.heroku.com/*",
    "http://wwww.browsing-history-visualizer.herokuapp.com/*",
    "http://*/",
    "http://0.0.0.0:5000/*",
    "http://browsing-history-visualizer.heroku.com/*",
    "http://wwww.browsing-history-visualizer.heroku.com/api/*",
    "http://wwww.browsing-history-visualizer.herokuapp.com/api/*",
    "http://127.0.0.1:5000/api"
  ]

 //"content_scripts": [
